    // main
    const nowTime = new Date().getTime();

    checkNewsClass();
    showExpireNews(publicShow, nowTime);
    publicShow = deleteExpireNews(publicShow, nowTime);

    renderNews(publicShow, 'all')

    for (key in newsClass) {
        let newsShow = publicShow.filter((item) => item.theClass && item.theClass === newsClass[key]);
        renderNews(newsShow, key)
    }

    // 放入 other 區塊
    // let newsShow = publicShow.filter((item) => !item.theClass || !isInClass(item.theClass))
    // renderNews(newsShow, 'other')

    tabsActive();

    // function
    function checkNewsClass() {
        for (let key in newsClass) {
            if (!document.querySelector('#' + key + 'Tab')) {
                buildTabAndPanel(key, newsClass[key]);
            }
        }
    }

    function buildTabAndPanel(key, value) {
        const nav = document.querySelector('#tabs-nav');
        const content = document.querySelector('#tabs-content');

        const tab = document.createElement('a');
        tab.className = 'tabs-menu';
        tab.id = key + 'Tab';
        tab.setAttribute('data-target', key);
        tab.href = '#';
        tab.innerText = value;
        nav.appendChild(tab);

        const panel = document.createElement('div');
        panel.id = key;
        panel.className = 'tabs-panel';

        content.appendChild(panel);
    }

    function showExpireNews(newsList, now) {
        // get expired news
        const timeOutShow = newsList.filter((item) => item.timeOut && timeFormate(item.timeOut) - now < 0);
        timeOutShow.forEach((item, index) => {
            console.log([
                index + 1 + '.',
                '    標題: ' + item.title,
                '    到期日: ' + item.timeOut,
                ' '
            ].join('\n'));
        });
    }

    function deleteExpireNews(newsList, now) {
        return newsList.filter((item) => !item.timeOut || timeFormate(item.timeOut) - now > 0);
    }

    function renderNews(newsList, targetPanelId) {
        const panel = document.querySelector('#' + targetPanelId);
        const table = document.createElement('table');
        table.border = 0;
        table.summary = '此表格僅供排版用';
        table.style.wordWrap = 'break-word';

        const tbody = document.createElement('tbody');
        let itemBool = true;

        for (let i = 0; i < newsList.length; i++) {
            const tr = document.createElement('tr');

            const index = document.createElement('td');
            index.width = '20';
            index.align = 'left';
            index.vAlign = 'top';

            if (targetPanelId === 'all') {
                index.innerText = newsList.length - i + '.';
            } else if (itemBool) {
                index.innerText = '○';
            } else {
                index.innerText = '●';
            }

            itemBool = !itemBool;
            
            tr.appendChild(index);

            const content = document.createElement('td');
            content.align = 'left';
            // content.className = 'news5link';

            const span = document.createElement('span');

            if (typeof (newsList[i].title) === 'object') {
                for (let j = 0; j < newsList[i].title.length; j++) {
                    if (newsList[i].link[j] === '') {
                        const regularText = document.createElement('span');

                        if (newsList[i].color[j] === '') {
                            regularText.style.color = 'black';
                        } else {
                            regularText.style.color = newsList[i].color[j];
                        }

                        regularText.innerText = newsList[i].title[j];
                        span.appendChild(regularText)
                    } else {
                        const a = document.createElement('a');
                        a.href = newsList[i].link[j];
                        a.target = '_blank';
                        // a.className = 'news5link style19';
                        a.className = 'newLinkStyle'

                        if (newsList[i].color[j] === '') {
                            a.style.color = 'red';
                        } else {
                            a.style.color = newsList[i].color[j];
                        }

                        a.innerText = newsList[i].title[j];
                        span.appendChild(a);
                    }
                }
            } else {
                const a = document.createElement('a');
                a.href = newsList[i].link;
                a.target = '_blank';
                // a.className = 'news5link style19';
                a.className = 'newLinkStyle'

                if (newsList[i].color) {
                    a.style.color = newsList[i].color;
                } else {
                    a.style.color = 'red';
                }

                a.innerText = newsList[i].title;
                span.appendChild(a);
            }

            content.appendChild(span);
            tr.appendChild(content);
            tbody.appendChild(tr);
        }

        table.appendChild(tbody);

        if (targetPanelId === 'school') {
            const constNews = document.querySelector('#constNews')
            panel.insertBefore(table, constNews);
            return
        }
        if (targetPanelId === 'all') {
            const constNews = document.querySelector('#constNewsall')
            panel.insertBefore(table, constNews);
            return
        }
        panel.appendChild(table);
    }

    function isInClass(classValue) {
        let flag = false
        for (let key in newsClass) {
            if (classValue === newsClass[key]) {
                flag = true
                break
            }
        }
        return flag
    }

    function timeFormate(timeString) {
        let year = null;
        let month = null;
        let date = null;
        let hour = null;
        let min = null;

        let ymdhm = timeString.split(' ');
        // console.log(ymdhm)
        if (ymdhm.length > 1) {
            let hm = ymdhm[1];
            if (hm.split(':').length > 1) {
                min = parseInt(hm.split(':')[1]);
            } else {
                min = 0;
            }
            hour = parseInt(hm.split(':')[0]);
        } else {
            hour = 23;
            min = 59;
        }

        let ymd = ymdhm[0].split('/');
        // console.log(ymd)
        year = parseInt(ymd[0]);
        month = parseInt(ymd[1]) - 1;
        date = parseInt(ymd[2]);
        // console.log(year, month, date, hour, min)

        return new Date(year, month, date, hour, min).getTime();
    }

    function tabsActive() {
        const tabMenus = Array.from(document.querySelectorAll('.tabs-menu'));
        tabMenus.forEach((tabMenu) => {
            tabMenu.onclick = jsTab;
        })

        function jsTab(e) {
            const nowTab = document.querySelector('.tabs-menu-active');
            const targetTab = e.target;

            const target = targetTab.getAttribute('data-target');
            const activePanel = document.querySelector('.tabs-panel.active');
            const targetPanel = document.querySelector('#' + target);
            activePanel.classList.remove('active');
            targetPanel.classList.add('active');

            nowTab.classList.remove('tabs-menu-active');
            targetTab.classList.add('tabs-menu-active');
        }
    }
</script>

