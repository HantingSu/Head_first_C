}

document.write("</tr></tbody></table>");
document.write("</div>");

function changePic(iPicNum){
	// clearTimeout(t);
	document.getElementById("posterPic").src="http://www.fhsh.tp.edu.tw/change/pic/picshow/"+ PICSHOW[iPicNum]+".jpg";
	document.getElementById("posterAnchor").href= PICSHOW_Link[iPicNum];
	document.getElementById("linkAnchor"+iPicNum).href = PICSHOW_Link[iPicNum];
}

function timeSlide(){
	if(bTrigger){
		changePic((currentPic++)%PICSHOW.length);
		setTimeout("timeSlide()",5000);
	}
}
 timeSlide();
